{
    "name": "hypernode/deployment",
    "description": "Deployment tools for Hypernode",
    "require": {
        "php": "^7.0",
        "symfony/console": "*",
        "monolog/monolog": "*",
        "symfony/yaml": "~2.8.7|~3.0.7|~3.1.1|~3.2",
        "psr/log": "*",
        "magento/magento-composer-installer": "*"
    },
    "type": "magento2-module",
    "version": "0.1.0",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Hypernode\\Deployment\\": "src/"
        }
    },
    "extra": {
        "map": [
            [
                "src/.hypernode.build.yml",
                ".hypernode.build.yml"
            ]
        ]
    }
}
